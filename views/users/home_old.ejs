<script>
  // Function to handle when the user tries to leave the page
  window.onbeforeunload = function(event) {
      // Check if the user is logged in (you need to implement this logic)
      var loggedIn = false; // Set to true if user is logged in
  
      // Check if the user is logged in and display a confirmation message
      if (loggedIn) {
          // Create a confirmation message (browser-specific)
          var message = 'Are you sure you want to leave this page?';
  
          // For some browsers
          if (typeof event === 'undefined') {
              event = window.event;
          }
          if (event) {
              event.returnValue = message;
          }
  
          // For others
          return message;
      }
  };
  </script>
